import { OnInit, EventEmitter, TemplateRef } from '@angular/core';
export declare class DatatableFooterComponent implements OnInit {
    template: TemplateRef<any>;
    datatable: any;
    pagerLeftArrowIcon?: String;
    pagerRightArrowIcon?: String;
    pagerPreviousIcon?: String;
    pagerNextIcon?: String;
    pageSizes?: any[];
    totalNumber?: Boolean;
    linage?: Boolean;
    displayBothEnds?: Boolean;
    pageNumber?: Boolean;
    page?: number;
    pageChange: EventEmitter<{}>;
    private _template;
    private _datatable;
    constructor();
    ngOnInit(): void;
    update(): void;
    emitPage(page: number): void;
}
