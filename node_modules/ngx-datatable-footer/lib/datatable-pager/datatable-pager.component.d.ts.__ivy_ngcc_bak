import { OnInit, EventEmitter, AfterViewInit, ElementRef, OnDestroy } from '@angular/core';
import { Subscription } from 'rxjs';
export declare class DatatablePagerComponent implements OnInit, AfterViewInit, OnDestroy {
    pagerLeftArrowIcon: string;
    pagerRightArrowIcon: string;
    pagerPreviousIcon: string;
    pagerNextIcon: string;
    pageSizes: any[];
    datatable: any;
    linage?: Boolean;
    displayBothEnds?: Boolean;
    pageNumber?: Boolean;
    size: number;
    count: number;
    page: number;
    readonly totalPages: number;
    change: EventEmitter<any>;
    pageChange: EventEmitter<any>;
    currentOffset: number;
    prveOffset: number;
    inputEvent: Subscription;
    customerInput: ElementRef;
    ngOnInit(): void;
    ngAfterViewInit(): void;
    _page: number;
    _count: number;
    _size: number;
    pages: any;
    pageSizeColumn: boolean;
    canPrevious(): boolean;
    canNext(): boolean;
    prevPage(): void;
    nextPage(): void;
    selectPage(page: number, changePageSize?: any): void;
    calcPages(page?: number): any[];
    valChange(event: any): void;
    blurHandel(): void;
    changePageSize(pageSize: number): void;
    keyDownHandel(event: any): boolean;
    ngOnDestroy(): void;
}
